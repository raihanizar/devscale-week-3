{
  "name": "raihan-tech",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "npx eslint **/*.js",
    "lintfix": "npx eslint **/*.js --fix",
    "dev": "concurrently \"lint\" \"vite\" \"npx tailwindcss -i ./src/css/styles.css -o ./src/css/global.css --watch\"",
    "build": "concurrently \"lintfix\" \"vite build\" \"npx tailwindcss -i ./src/css/styles.css -o ./src/css/global.css\"",
    "preview": "vite preview"
  },
  "devDependencies": {
    "canvas-confetti": "^1.9.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.8"
  }
}
